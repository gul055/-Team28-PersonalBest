<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ucsd.cse110.googlefitapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FriendProfileActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StepCountActivity" />
        <activity android:name=".Goals.SetGoalActivity" />
        <activity android:name=".Graph.GraphActivity" />
        <activity android:name=".Height.HeightPrompt" />
        <activity android:name=".Goals.promptGoal" />
        <activity
            android:name=".FriendViewActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".chatmessage.ChatActivity" />
    </application>

</manifest>